{
  "dashboard": {
    "title": "Jupyter Notebook Validator Operator - Overview",
    "tags": ["jupyter", "mlops", "operator", "notebooks"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Operator Health - Reconciliation Duration (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(notebookvalidationjob_reconciliation_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(notebookvalidationjob_reconciliation_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(notebookvalidationjob_reconciliation_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Validation Success Rate",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(notebookvalidationjob_validations_total{status=\"succeeded\"}[5m])) / sum(rate(notebookvalidationjob_validations_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 80, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Active Validation Pods",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(notebookvalidationjob_active_pods)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 20, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Error Rate by Type",
        "type": "graph",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(notebookvalidationjob_reconciliation_errors_total[5m])) by (error_type)",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Git Clone Performance",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(notebookvalidationjob_git_clone_duration_seconds_bucket[5m])) by (le, auth_type))",
            "legendFormat": "{{auth_type}} (p95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Validation Duration by Namespace",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(notebookvalidationjob_reconciliation_duration_seconds_bucket[5m])) by (le, namespace))",
            "legendFormat": "{{namespace}} (p95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Model Validation Duration by Platform",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(notebookvalidationjob_model_validation_duration_seconds_bucket[5m])) by (le, platform))",
            "legendFormat": "{{platform}} (p95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Model Health Checks",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(notebookvalidationjob_model_health_checks_total[5m])) by (platform, status)",
            "legendFormat": "{{platform}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Checks/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Platform Detection Duration",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(notebookvalidationjob_platform_detection_duration_seconds_bucket[5m])) by (le, platform))",
            "legendFormat": "{{platform}} (p95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "Prediction Validations",
        "type": "graph",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(notebookvalidationjob_prediction_validations_total[5m])) by (platform, result)",
            "legendFormat": "{{platform}} - {{result}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Validations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Pod Creation Success Rate",
        "type": "stat",
        "gridPos": {"x": 0, "y": 32, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(notebookvalidationjob_pod_creations_total{result=\"success\"}[5m])) / sum(rate(notebookvalidationjob_pod_creations_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 98, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Total Validations (24h)",
        "type": "stat",
        "gridPos": {"x": 6, "y": 32, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(notebookvalidationjob_validations_total[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Active Pods by Phase",
        "type": "graph",
        "gridPos": {"x": 12, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(notebookvalidationjob_active_pods) by (phase)",
            "legendFormat": "{{phase}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Pods"},
          {"format": "short"}
        ]
      }
    ]
  }
}

